{
  "name": "voice-order",
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "builds": [
    { 
      "src": "**/*", 
      "use": "@vercel/static" 
    }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "/$1" 
    }
  ]
} 